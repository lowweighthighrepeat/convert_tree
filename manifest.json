{
    "name": "Convert Tree",
    "version": "1.0",
    "description": "You can convert string to other encoding",
    "permissions": [
        "tabs",
        "activeTab"],
    "content_scripts": [{
        "css": [
            "css/sidenav.css"
        ],
        "js": [
            "js/init.js"
        ],
        "matches": ["*://*/*"]
    }],
    "web_accessible_resources": [
        "templates/*"
    ],
    "background": {
        "scripts": ["js/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "convert tree",
        "default_popup": "popup.html"
    },

    "manifest_version": 2
  }